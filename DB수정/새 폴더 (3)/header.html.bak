	<script>
	//스크롤로 메뉴위치확인
$(document).ready(function() {
	var jbOffset = $ ('nav').offset();
	$(window).scroll(function(){
		if($(document).scrollTop()>jbOffset.top) {
			$('nav').addClass('jbFixed');
			}
			else{
				$('nav').removeClass('jbFixed');
				}
				});
});
//로그인 창 팝업
function openForm() {
  document.getElementById("myForm").style.display = "block";
}

function closeForm() {
  document.getElementById("myForm").style.display = "none";
}


	</script>
	<style>
.open-button {
  background-color:#D6503E;
  color: white;
  border: none;
  cursor: pointer;
  opacity: 0.8;
  width: 110px;
  height: 30px;
  position:absolute;
  top:5px;
  right:80px;

}

	/*기본적으로 숨겨져있는 팝업창*/
.form-popup {
  display: none;
  position:absolute;
  top:300px;
  right:55%;
  width:400px;
  height:400px;
  border: 3px solid #f1f1f1;
  z-index: 9999999;
}

/* 폼 컨테이너에 스타일 추가 */
.form-container {
  max-width: 370px;
  padding: 10px;
  background-color: white;
}

/* 내부 입력창 풀사이즈*/
.form-container input[type=text], .form-container input[type=password] {
  width: 90%;
  padding: 15px;
  margin: 5px 0 22px 30px;
  border: none;
  background: #f1f1f1;
}

/* 입력창에 집중할때 클릭이 갈때 */
.form-container input[type=text]:focus, .form-container input[type=password]:focus {
  background-color: #ddd;
  outline: none;
}

/*서밋 로그인 버튼에 스타일 */
.form-container .btn {
  background-color: #4CAF50;
  color: white;
  padding: 16px 20px;
  border: none;
  cursor: pointer;
  width: 100%;
  margin-left:30px;
  margin-bottom:10px;
  margin-right:20px;
  opacity: 0.8;
  width: 110px;
  height: 50px;
 float:left;
}

/* Add a red background color to the cancel button */
.form-container .cancel {
  background-color: red;
  margin-right:40px; 
}

/* Add some hover effects to buttons */
.form-container .btn:hover, .open-button:hover {
  opacity: 1;
}
